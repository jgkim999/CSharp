{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Formatting": {
    "Localize":  false,
    "Number": {
      "Format": "n2",
      "Precision": 3
    }
  },
  "Database": {
    "Write": "Server=WriteServerAddress;Database=myDataBase;Uid=myUsername;Pwd=myPassword;MinimumPoolSize=10;maximumpoolsize=50;",
    "Read": "Server=ReadServerAddress;Database=myDataBase;Uid=myUsername;Pwd=myPassword;MinimumPoolSize=10;maximumpoolsize=50;"
  }
}
