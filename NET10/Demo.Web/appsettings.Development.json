{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "OpenTelemetry": "Information"
    }
  },
  "OpenTelemetry": {
    "ServiceName": "Demo.Web",
    "ServiceVersion": "1.0.0-dev",
    "Environment": "Development",
    "ServiceInstanceId": null,
    "Tracing": {
      "Enabled": true,
      "SamplingRatio": 1.0,
      "MaxSpans": 1000,
      "MaxAttributes": 64,
      "MaxEvents": 64,
      "MaxLinks": 64
    },
    "Metrics": {
      "Enabled": true,
      "CollectionIntervalSeconds": 15,
      "ExportTimeoutMilliseconds": 10000,
      "MaxBatchSize": 256,
      "BatchExportIntervalMilliseconds": 3000
    },
    "Logging": {
      "Enabled": true,
      "IncludeTraceId": true,
      "IncludeSpanId": true,
      "StructuredLogging": true
    },
    "Exporter": {
      "Type": "Console",
      "OtlpEndpoint": null,
      "OtlpHeaders": {},
      "OtlpProtocol": "grpc",
      "TimeoutMilliseconds": 5000,
      "BatchExport": {
        "MaxQueueSize": 1024,
        "MaxExportBatchSize": 256,
        "ExportTimeoutMilliseconds": 10000,
        "ScheduledDelayMilliseconds": 2000
      }
    }
  }
}
